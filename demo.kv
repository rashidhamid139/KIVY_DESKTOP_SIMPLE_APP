#: import TreeViewLabel kivy.uix.treeview.TreeViewLabel 

  

<Display>: 

    BoxLayout: 

        orientation: "vertical" 

         

        BoxLayout: 

            size_hint: 1, None 

            height: '48dp' 

            Button: 

                text: 'Migration' 

                on_release: sm.current = 'screen_one' 

                background_color: 0.93, 0.93, 0.93     

            Button: 

                text: 'Admin' 

                on_release: sm.current = 'screen_two' 

                background_color: 0.93, 0.93, 0.93 

        ScreenManager: 

            id: sm 

            Screen_One: 

            Screen_Two: 

  

<Screen_One>: 

    name: 'screen_one' 

    canvas.before: 

        Color: 

            rgba: 0.93, 0.93, 0.93, 0.8 

        Rectangle: 

            pos: self.pos 

            size: self.size         

  

    GridLayout: 

        padding: 10,10 

        cols:10 

        padding_top: 20 

        row_default_height:50 

        row_force_default: True 

        Label: 

            size_hint: None, None 

            height: 40 

            width: 40 

            foreground_color: 0,0,1,1 

            text: 'From:' 

        TextInput: 

            size_hint: None, None 

            height: 35 

            width: 150 

            id: start_field 

            multiline: False 

             

        Label: 

            size_hint: None, None 

            height: 40 

            width: 40 

            text: 'To:' 

            foreground_color: 0,0,1,1 

        TextInput: 

            size_hint: None, None 

            height: 35 

            width: 150 

            id: end_field 

            multiline: False 

             

        Label: 

            size_hint: None, None 

            height:40 

            width: 55 

            foreground_color: 0,0,1,1 

            text: 'Range:' 

        TextInput: 

            size_hint: None, None 

            height: 35 

            width: 150 

            id: range_field 

            multiline: False 

        Label: 

            text:  "" 

            size_hint: None, None 

            width:150 

  

             

        Button: 

            id: f1_but 

            text: 'Start' 

            size_hint: None, None 

            height: 40 

            width: 200 

            background_color: (0, 0.517, 0.705, 1) 

            pad_x:10 

            on_press: root.get_details() 

        Button: 

            id: f2_but 

            text: 'Clear' 

            size_hint: None,None 

            width: 200 

            height: 40 

            background_color: (1, 0.517, 0.705, 1)         

            on_press: root.clean() 

        Button: 

            id: f3_but 

            text: 'Exit' 

            size_hint: None, None 

            height: 40 

            width: 120 

            background_color: (1, 0.002, 0, 1) 

            on_press: root.Exit() 

         

                 

  

<Screen_Two>: 

    name: 'screen_two' 

    canvas.before: 

        Color: 

            rgba: 0.93, 0.93, 0.93 

        Rectangle: 

            pos: self.pos 

            size: self.size 

    BoxLayout: 

        width: 130 

        BoxLayout: 

            orientation: "vertical" 

            width: 150 

            size_hint_x: None 

            Button: 

                padding: 10,10 

                size_hint_y: None 

                height: '60' 

                text: 'Email' 

                halign: 'left' 

                on_release: sm1.current = 'screen_third' 

            Button: 

                padding: 10,10 

                size_hint_y: None 

                height: '60' 

                text: 'Subject' 

                halign: 'left' 

                on_release: sm1.current = 'screen_fourth' 

            Button: 

                padding: 10,10 

                size_hint_y: None 

                height: '60' 

                halign: 'left' 

                text: 'DataBase' 

                on_release: sm1.current='screen_fifth' 

            Button: 

                padding: 10,10 

                size_hint_y: None 

                height: '60' 

                halign: 'center' 

                text: 'Elasticsearch' 

                on_release: sm1.current = 'screen_six' 

            Button: 

                padding: 10,10 

                size_hint_y: None 

                height: '60' 

                text: 'Collections' 

                halign: 'left' 

                on_release: sm1.current = 'screen_seven' 

            Button: 

                padding: 10,10 

                size_hint_y: None 

                height: '60' 

                text: 'Parsing API' 

                halign: 'left' 

                on_release: sm1.current='screen_eight' 

                 

            Button: 

             

             

        ScreenManager: 

            id: sm1 

